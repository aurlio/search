{
  "manifest_version": 2,

  "name": "Advanced Search",
  "description": "This extension allows search with Case-sensitive and words",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
  ],
  "content_scripts":[
	{
	"matches":["http://*/*","https://*/*"],
	"js":["content.js"],
	"run_at":"document_end"
	}
  ]
}
